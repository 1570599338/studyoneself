            </if>
            <if test="lastAccessTime != null">
                last_access_time = #{lastAccessTime},
            </if>
            <if test="expireTime != null">
                expire_time = #{expireTime},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from t_user_online where id = #{id}
    </delete>

</mapper>

